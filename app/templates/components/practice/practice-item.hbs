<section class="practice-header">
  <div class="container">
    <div class="row">
      <div class="col-md-12 text-center">
        <img src="{{item.image_url}}" alt="" class="img-fluid" style="width: 100%; height: auto;">
        <div class="practice-title">{{item.title}}</div>
        <div class="practice-category">{{t "practices"}}</div>
      </div>
    </div>
  </div>
</section>

<section class="practice-body">
  <div class="container cont2-practice">
    <div class="row">
      <div class="col-md-4 col-sm-3">
        <div class="practice-time disp_n_768">
          <i class="fa fa-clock-o"></i> {{moment-format item.created "DD.MM.YYYY"}}
        </div>
      </div>
      <div class="col-md-7 col-sm-9">
        <div class="block-practice-right">
          {{{item.description}}}
        </div>
      </div>
    </div>
  </div>
  <div class="block-share" id="block-share-practice" style="height: 144px; padding: 0px; border: 0px;">
    <div class=""
         style="background: none 0% 0% / auto repeat scroll padding-box border-box rgba(0, 0, 0, 0); background-blend-mode: normal; border: 0px none rgb(51, 51, 51); border-radius: 0px; border-collapse: separate; box-shadow: none; outline: rgb(51, 51, 51) none 0px; outline-offset: 0px; overflow-anchor: auto; overflow-wrap: normal; overflow: visible; padding: 0px; box-sizing: border-box; width: 48px;">
      <div class="box-f-share">
        <a href="f-share"><img src="/images/f-share.png" alt=""></a>
        <span>241</span>
      </div>
      <div class="box-tw-share">
        <a href="tw-share"><img src="/images/tw-share.png" alt=""></a>
        <span>0</span>
      </div>
    </div>
  </div>
</section>